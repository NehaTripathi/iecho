<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/relativeLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:background="#ffffff" xmlns:android="http://schemas.android.com/apk/res/android"
	android:padding="5dip">

	<TextView android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_height="wrap_content" android:text="TextView"
		android:layout_centerVertical="true" android:layout_width="150dip"
		android:id="@+id/contact_txtViewTitle" android:textColor="#000000"
		android:layout_marginLeft="2dip">
	</TextView>

	<ImageView android:layout_width="20dip" android:visibility="gone"
		android:layout_centerInParent="true" android:layout_height="20dip"
		android:id="@+id/contact_imgViewReject" android:src="@drawable/icon"
		android:layout_below="@+id/contact_txtViewDescription3"
		android:layout_toRightOf="@id/contact_txtViewTitle"
		android:layout_marginLeft="25dip">
	</ImageView>

	<ImageView android:layout_width="20dip" android:visibility="gone"
		android:layout_height="20dip" android:id="@+id/contact_imgViewAccept"
		android:src="@drawable/icon" android:layout_below="@+id/contact_txtViewDescription3"
		android:layout_centerInParent="true" android:layout_toRightOf="@id/contact_imgViewReject"
		android:layout_marginLeft="20dip">
	</ImageView>

	<ImageView android:layout_width="50dip" android:visibility="gone"
		android:layout_height="50dip" android:id="@+id/contact_imgViewLogo"
		android:src="@drawable/icon" android:layout_alignParentRight="true"
		android:layout_centerInParent="true" android:scaleType="center">
	</ImageView>
</RelativeLayout> 










